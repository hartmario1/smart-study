#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mario/code/soulkiller/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/bin/node_modules:/home/mario/code/soulkiller/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/node_modules:/home/mario/code/soulkiller/node_modules/.pnpm/typescript@4.4.4/node_modules:/home/mario/code/soulkiller/node_modules/.pnpm/node_modules:/home/mario/code/soulkiller/node_modules:/home/mario/code/node_modules:/home/mario/node_modules:/home/node_modules:/node_modules:/home/mario/code/soulkiller/libs/rest/node_modules:/home/mario/code/soulkiller/libs/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/mario/code/soulkiller/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/bin/node_modules:/home/mario/code/soulkiller/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/node_modules:/home/mario/code/soulkiller/node_modules/.pnpm/typescript@4.4.4/node_modules:/home/mario/code/soulkiller/node_modules/.pnpm/node_modules:/home/mario/code/soulkiller/node_modules:/home/mario/code/node_modules:/home/mario/node_modules:/home/node_modules:/node_modules:/home/mario/code/soulkiller/libs/rest/node_modules:/home/mario/code/soulkiller/libs/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/bin/tsserver" "$@"
fi
